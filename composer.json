{
    "name": "laravel-notification-channels/smsc-ru",
    "description": "SmscRu Notifications driver.",
    "keywords": ["laravel", "notifications", "smscentre", "smsc", "СМС-Центр"],
    "homepage": "https://github.com/laravel-notification-channels/smsc-ru",
    "license": "MIT",
    "authors": [
        {
            "name": "JhaoDa",
            "role": "Developer",
            "email": "jhaoda@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/events": "^5.3@dev",
        "illuminate/notifications": "^5.3@dev",
        "illuminate/queue": "^5.3@dev",
        "illuminate/support": "^5.3@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\SmscRu\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\SmscRu\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev"
}
